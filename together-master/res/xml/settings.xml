<?xml version="1.0" encoding="UTF-8"?><PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >    <PreferenceCategory android:title="我的个人信息" >        <CheckBoxPreference            android:key="my_gps_satellite_setting"            android:summary="定位时，精确到街道级别（取消选择可节约电量）"            android:title="启用GPS卫星设置" />        <CheckBoxPreference            android:key="my_wireless_network"            android:summary="使用无线网络查看应用程序（例如Google地图）中的位置"            android:title="使用无线" />    </PreferenceCategory>    <PreferenceCategory android:title="个人信息设置" >        <CheckBoxPreference            android:key="my_yesno_save_individual_info"            android:title="是否保存个人信息" />        <EditTextPreference            android:key="my_individual_name"            android:summary="请输入真实姓名"            android:title="姓名" />        <PreferenceScreen            android:key="my_other_individual_msg"            android:summary="是否工作、单位名称"            android:title="其他个人信息" >            <CheckBoxPreference                android:key="my_is_an_employee"                android:title="是否工作" />            <EditTextPreference                android:key="my_comp"                android:summary="请输入您的单位名称"                android:title="单位名称" />        </PreferenceScreen>    </PreferenceCategory></PreferenceScreen>